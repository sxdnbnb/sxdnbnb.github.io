#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/桌面/博客/sxdnbnb.github.io/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.0_async-validator@4.2.5_postcss@8.5.3_sass-embedd_dsncb5tsfj2ma5nq4bq57pxu6a/node_modules/vitepress/bin/node_modules:/mnt/d/桌面/博客/sxdnbnb.github.io/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.0_async-validator@4.2.5_postcss@8.5.3_sass-embedd_dsncb5tsfj2ma5nq4bq57pxu6a/node_modules/vitepress/node_modules:/mnt/d/桌面/博客/sxdnbnb.github.io/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.0_async-validator@4.2.5_postcss@8.5.3_sass-embedd_dsncb5tsfj2ma5nq4bq57pxu6a/node_modules:/mnt/d/桌面/博客/sxdnbnb.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/桌面/博客/sxdnbnb.github.io/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.0_async-validator@4.2.5_postcss@8.5.3_sass-embedd_dsncb5tsfj2ma5nq4bq57pxu6a/node_modules/vitepress/bin/node_modules:/mnt/d/桌面/博客/sxdnbnb.github.io/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.0_async-validator@4.2.5_postcss@8.5.3_sass-embedd_dsncb5tsfj2ma5nq4bq57pxu6a/node_modules/vitepress/node_modules:/mnt/d/桌面/博客/sxdnbnb.github.io/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.23.0_async-validator@4.2.5_postcss@8.5.3_sass-embedd_dsncb5tsfj2ma5nq4bq57pxu6a/node_modules:/mnt/d/桌面/博客/sxdnbnb.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
